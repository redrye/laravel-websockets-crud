{
    "name": "redrye/laravel-websockets-crud",
    "description": "Broadcast create, read, update, and delete events for realtime updates such as Datatables or custom information boxes",
    "require": {
        "beyondcode/laravel-websockets": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Redrye\\LaravelWebSocketsCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redrye\\LaravelWebSocketsCrud\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redrye\\LaravelWebsocketsCrud\\CrudEventsServiceProvider"
            ]
        }
    }
}
